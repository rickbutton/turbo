{
  "name": "turbo",
  "version": "0.2.1",
  "license": "MIT",
  "author": "Rick Button <rick@button.dev>",
  "homepage": "https://github.com/rickbutton/turbo",
  "description": "the turbo command line application",
  "main": "lib/index",
  "types": "lib/index",
  "files": [
    "lib",
    "index.js"
  ],
  "bin": {
    "turbo": "./index.js"
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./lib && rm -rf tsconfig.build.tsbuildinfo",
    "compile": "tsc -b tsconfig.build.json",
    "compile-watch": "tsc -w -b tsconfig.build.json",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^12.12.5"
  },
  "dependencies": {
    "@turbo/connector-node": "^0.2.0",
    "@turbo/core": "^0.2.0",
    "@turbo/net": "^0.2.0",
    "@turbo/shell-tmux": "^0.2.0",
    "@types/react": "^16.9.16",
    "@types/react-reconciler": "^0.18.0",
    "@types/terminal-kit": "^1.28.0",
    "@types/yargs": "^13.0.3",
    "@types/yoga-layout": "^1.9.1",
    "cli-highlight": "^2.1.4",
    "react": "^16.12.0",
    "react-reconciler": "^0.24.0",
    "string-width": "^4.2.0",
    "terminal-kit": "^1.32.2",
    "yargs": "^14.2.0",
    "yoga-layout-prebuilt": "^1.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
