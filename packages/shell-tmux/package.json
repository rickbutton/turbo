{
  "name": "@turbo/shell-tmux",
  "version": "0.2.0",
  "license": "MIT",
  "author": "Rick Button <rick@button.dev>",
  "homepage": "https://github.com/rickbutton/turbo",
  "description": "tmux support for the turbo debugger",
  "main": "lib/index",
  "types": "lib/index",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./lib && rm -rf tsconfig.build.tsbuildinfo",
    "compile": "tsc -b tsconfig.build.json",
    "compile-watch": "tsc -w -b tsconfig.build.json",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/ffi": "^0.2.2",
    "@types/ref": "^0.0.28",
    "@types/ref-array": "^0.0.28"
  },
  "dependencies": {
    "@turbo/core": "^0.2.0",
    "ffi": "github:lxe/node-ffi#node-12",
    "ref": "github:lxe/ref#node-12",
    "ref-array": "github:lxe/ref-array#node-12"
  },
  "publishConfig": {
    "access": "public"
  }
}
