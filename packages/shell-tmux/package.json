{
  "name": "@turbo/shell-tmux",
  "version": "0.2.3",
  "license": "MIT",
  "author": "Rick Button <rick@button.dev>",
  "homepage": "https://github.com/rickbutton/turbo",
  "description": "tmux support for the turbo debugger",
  "main": "lib/index",
  "types": "lib/index",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./lib && rm -rf tsconfig.build.tsbuildinfo",
    "compile": "tsc -b tsconfig.build.json",
    "compile-watch": "tsc -w -b tsconfig.build.json",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/ffi-napi": "^2.4.1",
    "@types/ref-array-di": "^1.2.0",
    "@types/ref-napi": "^1.4.0"
  },
  "dependencies": {
    "@turbo/core": "^0.2.3",
    "ffi-napi": "^2.4.6",
    "ref-array-di": "^1.2.1",
    "ref-napi": "^1.4.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
